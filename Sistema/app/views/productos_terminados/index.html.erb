<h1>Listing productos_terminados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Codigo</th>
      <th>Descripcion</th>
      <th>Cantidad minima</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @productos_terminados.each do |productos_terminado| %>
      <tr>
        <td><%= productos_terminado.nombre %></td>
        <td><%= productos_terminado.codigo %></td>
        <td><%= productos_terminado.descripcion %></td>
        <td><%= productos_terminado.cantidad_minima %></td>
        <td><%= link_to 'Show', productos_terminado %></td>
        <td><%= link_to 'Edit', edit_productos_terminado_path(productos_terminado) %></td>
        <td><%= link_to 'Destroy', productos_terminado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Productos terminado', new_productos_terminado_path %>
