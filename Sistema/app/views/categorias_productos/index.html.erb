<h1>Listing categorias_productos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categorias_productos.each do |categoria_producto| %>
      <tr>
        <td><%= categoria_producto.nombre %></td>
        <td><%= categoria_producto.descripcion %></td>
        <td><%= link_to 'Show', categoria_producto %></td>
        <td><%= link_to 'Edit', edit_categoria_producto_path(categoria_producto) %></td>
        <td><%= link_to 'Destroy', categoria_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categoria producto', new_categoria_producto_path %>
