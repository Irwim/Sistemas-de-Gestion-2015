<h1>Listing movimientos_componentes</h1>

<table>
  <thead>
    <tr>
      <th>Deposito</th>
      <th>Componente</th>
      <th>Tipo de movimiento</th>
      <th>Fecha</th>
      <th>Cantidad transferida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movimientos_componentes.each do |movimiento_componente| %>
      <tr>
        <td><%= movimiento_componente.deposito %></td>
        <td><%= movimiento_componente.componente %></td>
        <td><%= movimiento_componente.tipo_de_movimiento %></td>
        <td><%= movimiento_componente.fecha %></td>
        <td><%= movimiento_componente.cantidad_transferida %></td>
        <td><%= link_to 'Show', movimiento_componente %></td>
        <td><%= link_to 'Edit', edit_movimiento_componente_path(movimiento_componente) %></td>
        <td><%= link_to 'Destroy', movimiento_componente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movimiento componente', new_movimiento_componente_path %>
