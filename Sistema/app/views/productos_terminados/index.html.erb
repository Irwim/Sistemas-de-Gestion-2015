<h1>Listing productos_terminados</h1>

<table>
  <thead>
    <tr>
      <th>Modelo</th>
      <th>Categoria producto</th>
      <th>Codigo</th>
      <th>Cantidad minima</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @productos_terminados.each do |producto_terminado| %>
      <tr>
        <td><%= producto_terminado.modelo %></td>
        <td><%= producto_terminado.categoria_producto %></td>
        <td><%= producto_terminado.codigo %></td>
        <td><%= producto_terminado.cantidad_minima %></td>
        <td><%= producto_terminado.descripcion %></td>
        <td><%= link_to 'Show', producto_terminado %></td>
        <td><%= link_to 'Edit', edit_producto_terminado_path(producto_terminado) %></td>
        <td><%= link_to 'Destroy', producto_terminado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Producto terminado', new_producto_terminado_path %>
