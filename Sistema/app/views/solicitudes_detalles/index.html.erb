<h1>Listing solicitudes_detalles</h1>

<table>
  <thead>
    <tr>
      <th>Solicitud monitor teclado</th>
      <th>Componente</th>
      <th>Cantidad solicitada</th>
      <th>Cantidad entregada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitudes_detalles.each do |solicitud_detalle| %>
      <tr>
        <td><%= solicitud_detalle.solicitud_monitor_teclado %></td>
        <td><%= solicitud_detalle.componente %></td>
        <td><%= solicitud_detalle.cantidad_solicitada %></td>
        <td><%= solicitud_detalle.cantidad_entregada %></td>
        <td><%= link_to 'Show', solicitud_detalle %></td>
        <td><%= link_to 'Edit', edit_solicitud_detalle_path(solicitud_detalle) %></td>
        <td><%= link_to 'Destroy', solicitud_detalle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud detalle', new_solicitud_detalle_path %>
