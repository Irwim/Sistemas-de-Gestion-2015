<h1>Listing tipos_de_depositos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_de_depositos.each do |tipo_de_deposito| %>
      <tr>
        <td><%= tipo_de_deposito.nombre %></td>
        <td><%= link_to 'Show', tipo_de_deposito %></td>
        <td><%= link_to 'Edit', edit_tipo_de_deposito_path(tipo_de_deposito) %></td>
        <td><%= link_to 'Destroy', tipo_de_deposito, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo de deposito', new_tipo_de_deposito_path %>
