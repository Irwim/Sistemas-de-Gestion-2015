<h1>Listing solicitudes_componentes</h1>

<table>
  <thead>
    <tr>
      <th>Deposito</th>
      <th>Deposito</th>
      <th>Fecha</th>
      <th>Nro formulario</th>
      <th>Fecha baja</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitudes_componentes.each do |solicitud_componente| %>
      <tr>
        <td><%= solicitud_componente.deposito %></td>
        <td><%= solicitud_componente.deposito %></td>
        <td><%= solicitud_componente.fecha %></td>
        <td><%= solicitud_componente.nro_formulario %></td>
        <td><%= solicitud_componente.fecha_baja %></td>
        <td><%= solicitud_componente.estado %></td>
        <td><%= link_to 'Show', solicitud_componente %></td>
        <td><%= link_to 'Edit', edit_solicitud_componente_path(solicitud_componente) %></td>
        <td><%= link_to 'Destroy', solicitud_componente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

