<h1>Listing movimientos_productos</h1>

<table>
  <thead>
    <tr>
      <th>Deposito</th>
      <th>Producto terminado</th>
      <th>Tipo de movimiento</th>
      <th>Fecha</th>
      <th>Cantidad transferida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movimientos_productos.each do |movimiento_producto| %>
      <tr>
        <td><%= movimiento_producto.deposito %></td>
        <td><%= movimiento_producto.producto_terminado %></td>
        <td><%= movimiento_producto.tipo_de_movimiento %></td>
        <td><%= movimiento_producto.fecha %></td>
        <td><%= movimiento_producto.cantidad_transferida %></td>
        <td><%= link_to 'Show', movimiento_producto %></td>
        <td><%= link_to 'Edit', edit_movimiento_producto_path(movimiento_producto) %></td>
        <td><%= link_to 'Destroy', movimiento_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movimiento producto', new_movimiento_producto_path %>
